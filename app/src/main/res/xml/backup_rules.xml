<?xml version="1.0" encoding="utf-8"?>
<!--
   Backup rules for Android 11 and lower (API < 31).
   Configures what data is included in Auto Backup.
-->
<full-backup-content>
    <!-- Include files, databases, and shared prefs -->
    <!-- Note: Cache files are automatically excluded by the OS -->
    <include domain="file" path="."/>
    <include domain="database" path="."/>
    <include domain="sharedpref" path="."/>
</full-backup-content>